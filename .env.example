# Kopiraj v .env in po potrebi spremeni

# Lokalni node
RPC_URL=http://127.0.0.1:8545

# Naslov pogodbe (po deployu se bo auto-dodal)
CONTRACT_ADDRESS=

# Uporabi prvi anvil key (default)
PRIVATE_KEY=0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80

# Namespace iz ontologije
NAMESPACE=http://example.org/trust#

# Lokacije datotek
OWL_PATH=ontologies/pharma-trust.owl
POLICIES_DIR=policies
RESULTS_DIR=results

# Batch velikost pri zapisu (po potrebi)
BATCH_SIZE=200

# Identitetni način: URI ali DID (če boš kasneje uporabljal DID)
IDENTITY_MODE=URI
